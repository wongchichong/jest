{
  "name": "jest-each",
  "version": "29.5.0",
  "description": "Parameterised tests for Jest",
  "main": "./build/index.cjs.js",
  "module": "./build/index.es.js",
  "browser": "./build/index.umd.js",

  "types": "./build/index.d.ts",
